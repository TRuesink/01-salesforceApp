ist = path.filter(\n      (item) => item.picklistValueName === this.props.currentStage\n    );\n    if (assist.length === 0) {\n      return <div>Add Instructions to Path Assist in your Salesforce Org</div>;\n    }\n    return <div dangerouslySetInnerHTML={{ __html: assist[0].info }}></div>;\n  }\n\n  onClickStage = (stage) => {\n    this.props.onSubmit({ StageName: stage });\n    this.props.changeLoadingStatus(LOADING);\n  };\n\n  renderSteps() {\n    const { stages, currentStage } = this.props;\n    let activeIndex = 100;\n    return stages.map((stage, index) => {\n      if (stage.value === currentStage) {\n        activeIndex = index;\n      }\n      return (\n        <a\n          onClick={() => this.onClickStage(stage.value)}\n          href=\"#\"\n          key={stage.value}\n          className={\n            index < activeIndex\n              ? \"completed step\"\n              : index > activeIndex\n              ? \"step\"\n              : \"active step\"\n          }\n        >\n          <div className=\"content\">\n            <div className=\"title\">{stage.value}</div>\n            <div className=\"description\"></div>\n          </div>\n        </a>\n      );\n    });\n  }\n  renderContent() {\n    const { stages } = this.props;\n    return (\n      <div className=\"ui two column grid\">\n        <div className=\"six wide column\">\n          <div\n            className={\n              this.props.loadingStatus\n                ? \"ui loading basic segment\"\n                : \"ui basic segment\"\n            }\n          >\n            <div\n              className={`ui ${converter.toWords(\n                stages.length\n              )} fluid mini vertical ordered steps`}\n            >\n              {this.renderSteps()}\n            </div>\n          </div>\n        </div>\n        <div className=\"ten wide column\">\n          <div style={{ height: \"100%\" }} className=\"ui segment\">\n            <h3>Directions</h3>\n            <div className=\"ui clearing divider\"></div>\n            {this.renderStepAssist()}\n          </div>\n        </div>\n      </div>\n    );\n  }\n\n  render() {\n    return <div>{this.renderContent()}</div>;\n  }\n}\n\nconst mapStateToProps = (state) => {\n  return {\n    metadata: state.metadata,\n    loadingStatus: state.loadingStatus,\n  };\n};\n\nexport default connect(mapStateToProps, { fetchPaths, changeLoadingStatus })(\n  Path\n);\n","/Users/timruesink/Projects/Portfolio/01-salesforceApp/client/src/reducers/editModeReducer.js",[],"/Users/timruesink/Projects/Portfolio/01-salesforceApp/client/src/components/Pagination.js",["157","158"],"import React from \"react\";\n\nclass Pagination extends React.Component {\n  renderPrev() {\n    const { pagination } = this.props;\n    if (!pagination.prev) {\n      return;\n    }\n    return (\n      <a\n        onClick={() => this.props.onPageTurn(pagination.prev)}\n        className=\"item\"\n      >\n        {pagination.prev.page}\n      </a>\n    );\n  }\n  renderNext() {\n    const { pagination } = this.props;\n    if (!pagination.next) {\n      return;\n    }\n    return (\n      <a onClick={() => this.onPageClick(pagination.next)} className=\"item\">\n        {pagination.next.page}\n      </a>\n    );\n  }\n\n  onPageClick(pagination) {\n    this.props.onPageTurn(pagination);\n  }\n\n  renderCurrent() {\n    const { pagination } = this.props;\n    if (!pagination.next && !pagination.prev) {\n      return <div className=\"active item\">1</div>;\n    } else if (!pagination.prev) {\n      return <div className=\"active item\">{pagination.next.page - 1}</div>;\n    } else {\n      return <div className=\"active item\">{pagination.prev.page + 1}</div>;\n    }\n  }\n\n  render() {\n    return (\n      <div className=\"ui basic segment center aligned\">\n        <div className=\"ui pagination menu\">\n          {this.renderPrev()}\n          {this.renderCurrent()}\n          {this.renderNext()}\n        </div>\n      </div>\n    );\n